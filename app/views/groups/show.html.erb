<h3>
  Total amount: <%= @group_contracts.sum { |gc| gc.contract.amount } %>
</h3>
<ul>
  <% @group_contracts.each do |group_contract| %>
    <li>
      <div>
        <div>
        </div>
        <h4><%= group_contract.contract.name.capitalize %></h4>
        <h4><%= group_contract.contract.amount %></h4>
        <h4><%= group_contract.contract.created_at %></h4>
      </div>
    </li>
  <% end %>
</ul>
